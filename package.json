{
  "name": "angular-template",
  "version": "1.0",
  "description": "Angular Template",
  "author": "Cesar Vargas (@CesarVargasDev)",
  "keywords": [
    "angular",
    "template",
    "skeleton"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "start:dev": "ng serve --configuration development --port 4200 --open",
    "start:prod": "ng serve --configuration production --port 4300 --open",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production && npm run build:optimize",
    "optimize:i18n": "transloco-optimize dist/browser/assets/i18n",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:extract-unflat": "transloco-keys-manager extract --unflat",
    "i18n:find": "transloco-keys-manager find",
    "check:types": "tsc --pretty --noEmit && tsc --project cypress/tsconfig.json --pretty --noEmit",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext .js,.ts --color",
    "check:i18n": "transloco-validator src/assets/i18n/*.json",
    "check:staged": "lint-staged",
    "fix:format": "prettier --check --write --ignore-unknown .",
    "fix:lint": "npm run check:lint -- --fix",
    "e2e": "ng e2e",
    "e2e:run": "ng run angular-template:cypress-run",
    "e2e:open": "ng run angular-template:cypress-open",
    "e2e:coverage:view": "http-server -p 9004 ./coverage-e2e/lcov-report -o",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage --silent",
    "test:coverage:view": "http-server -p 9003 ./coverage/lcov-report -o",
    "prepare": "husky",
    "lint": "ng lint",
    "test": "jest",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^18.0.5",
    "@angular/animations": "^18.0.5",
    "@angular/common": "^18.0.5",
    "@angular/compiler": "^18.0.5",
    "@angular/core": "^18.0.5",
    "@angular/forms": "^18.0.5",
    "@angular/platform-browser": "^18.0.5",
    "@angular/platform-browser-dynamic": "^18.0.5",
    "@angular/router": "^18.0.5",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@jsverse/transloco": "^7.4.2",
    "@jsverse/transloco-keys-manager": "^4.2.2",
    "@jsverse/transloco-optimize": "^7.0.1",
    "@jsverse/transloco-validator": "^7.0.1",
    "@ngneat/until-destroy": "^10.0.0",
    "bootstrap": "^5.3.3",
    "ngx-bootstrap": "^18.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^18.0.0",
    "@angular-devkit/build-angular": "^18.0.5",
    "@angular-eslint/builder": "^18.1.0",
    "@angular-eslint/eslint-plugin": "^18.1.0",
    "@angular-eslint/eslint-plugin-template": "^18.1.0",
    "@angular-eslint/schematics": "18.1.0",
    "@angular-eslint/template-parser": "18.1.0",
    "@angular/build": "^18.0.5",
    "@angular/cli": "^18.0.5",
    "@angular/compiler-cli": "^18.0.5",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@cypress/schematic": "^2.5.2",
    "@eslint/js": "^9.6.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/dotenv-defaults": "^2.0.4",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "angular-eslint": "18.1.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "get-tsconfig": "^4.7.5",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "typescript": "~5.4.2"
  }
}
